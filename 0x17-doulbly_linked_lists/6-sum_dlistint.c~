#include "lists.h"

/**
 * sum_dlistint - calculates the sum of all the data (n) if a doubly linked list
 * @head: pointer to head node
 *
 * Return: integer result
 */
int sum_dlistint(dlistint_t *head)
{
	dlistint_t *node = head;
	int result = 0;

	while (node)
	{
		node = node->next;
		result += node->n
	}
	return (result);
}
