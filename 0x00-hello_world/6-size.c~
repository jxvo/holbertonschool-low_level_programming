#include <stdio.h>

/**
 * main - print type sizes
* Description: prints the size of various types on the computer
* Return: returns zero
*/

int main(void)
{
int integerType;
float floatType;
long longType;

printf("Size of a char: %ld byte(s)\n",sizeof(integerType);
printf("Size of an int: %ld byte(s)\n",sizeof(floatType);
printf("Size of a long int: %ld byte(s)\n", sizeof(longType+longType+integerType);
printf("Size of a float: %ld byte(s)\n", sizeof(floatType);
}
